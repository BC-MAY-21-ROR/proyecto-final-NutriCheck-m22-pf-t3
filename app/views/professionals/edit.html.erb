<h1>Edit Professional</h1>
<div>
  <%= form_with model: @user do |form| %>
    <div>
      <%= form.label :name %><br>
      <%= form.text_field :name  %>
    </div>
    <div>
      <%= form.label :last_name %><br>
      <%= form.text_field :last_name %>
    </div>
    <div>
      <%= form.label :birth_date %><br>
      <%= form.text_field :birth_date %>
    </div>
    <div>
      <%= form.label :phone %><br>
      <%= form.text_field :phone %>
    </div>
    <div>
      <%= form.label :email %><br>
      <%= form.text_field :email%>
    </div>
    <div>
      <%= form.label :password %><br>
      <%= form.text_field :password%>
    </div>
    <div>
      <%= form.label :role %><br>
      <%= form.text_field :role%>
    </div>
    <div>
      <%= form.label :speciality_id %><br>
      <%= form.text_field :speciality_id%>
    </div>
    <div>
      <%= form.submit 'Edit'%>
    </div>
  <% end %>
</div>
