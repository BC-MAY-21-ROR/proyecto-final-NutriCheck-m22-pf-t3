<h1>Manage specialities</h1>
<div>
  <% @specialities.each do |speciality|%>
    <div>
      <div>
        <%= speciality.name %>
      </div>
      <div>
        <%= button_to "Edit", edit_speciality_path(speciality), method: :get %>
        <%= button_to "Destroy", speciality_path(speciality), method: :delete %>
      </div>
      <hr>
    <% end %>
  </div>
</div>
<div>
  <%= button_to "New Speciality", new_speciality_path, method: :get %>
</div>
<div>
  <%= button_to "Go back", professionals_path, method: :get %>
</div>
